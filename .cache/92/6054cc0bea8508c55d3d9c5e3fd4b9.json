ProductList.js","original":{"line":47,"column":23}},{"generated":{"line":131,"column":49},"source":"src/component/ProductList.js","original":{"line":47,"column":4}},{"generated":{"line":132,"column":0},"source":"src/component/ProductList.js","original":{"line":48,"column":3}},{"name":"Observer","generated":{"line":136,"column":2},"source":"src/component/ProductList.js","original":{"line":3,"column":26}},{"name":"ProductList","generated":{"line":138,"column":15},"source":"src/component/ProductList.js","original":{"line":51,"column":15}}],"sources":{"src/component/ProductList.js":"import Observer from '../lib/Observer';\n\nclass ProductList extends Observer {\n  constructor(state = {}) {\n    super();\n    this.appState = state;\n  }\n\n  createMarkup(state) {\n    return `<div>${state.products.map(product => `<button id=\"addBasket\" data-id=\"${product.id}\">${product.name}</button>`)}</div>`;\n  }\n\n  render(state, selector = \"app\") {\n    const markup = this.createMarkup(state);\n    const parent = document.getElementById(selector);\n  \n    parent.innerHTML = markup;\n    this.clickEvent();\n  }\n\n  clickEvent() {\n    const products = document.querySelectorAll('#addBasket');\n    for (let button of products) {\n      button.addEventListener('click', (e) => {\n        e.preventDefault();\n        const state = this.appState.getState();\n        const name = e.target.innerText;\n        const id = e.target.getAttribute('data-id');\n  \n        if (!name || state.baskets.find(basket => basket.id === id)) {\n          return;\n        }\n  \n        // Getting the current state.\n        const baskets = [...state.baskets, { id, name }];\n  \n        // Updating state will prompt a re-render via the notify method being called.\n        this.appState.update({\n          ...state,\n          baskets\n        });\n      })\n    }\n  }\n\n  update(state) {\n    this.render(state, \"product-list-container\");\n  }\n}\n\nexport default ProductList;"},"lineCount":null}}